<Defs>
  <!-- Militech uniform -->
  <ThingDef ParentName="ApparelBase">
    <defName>CP_MBodySuit</defName>
    <label>Militech jumpsuit</label>
    <description>This dark grey jumpsuit is the standard uniform for Militech soldiers.</description>
    <graphicData>
      <texPath>CP/Apparel/Gibson/GB_Bodysuit</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costList>
      <Hyperweave>15</Hyperweave>
    </costList>
    <statBases>
      <MarketValue>300</MarketValue>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>1.38</Mass>
      <ArmorRating_Blunt>0.10</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.10</ArmorRating_Sharp>
      <ArmorRating_Heat>0.10</ArmorRating_Heat>
      <Insulation_Cold>7</Insulation_Cold>
      <EquipDelay>5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>0.00</MoveSpeed>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>CP/Apparel/Gibson/GB_Bodysuit</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>Militech</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>5</weight>
          <only>RGBA(0.3,0.3,0.3,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
